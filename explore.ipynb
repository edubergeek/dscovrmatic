   "source": [
    "# Time Conversion functions\n",
    "\n",
    "Let's define some helper functions to convert some time formats into human readable time"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "3beb4cb8",
   "metadata": {},
   "outputs": [],
   "source": [
    "def time_from_seconds(seconds):\n",
    "  seconds = seconds % (24 * 3600)\n",
    "  hour = seconds // 3600\n",
    "  seconds %= 3600\n",
    "  minutes = seconds // 60\n",
    "  seconds %= 60\n",
    "     \n",
    "  return \"%d:%02d:%02d\" % (hour, minutes, seconds)\n",
    "\n",
    "def time_from_doy(doy):\n",
    "  part, whole = np.modf(doy)\n",
    "  seconds = part * 86400\n",
    "     \n",
    "  return time_from_seconds(seconds)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "e46010fb",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "3600 seconds: 1:00:00\n",
      "0.04167 days: 1:00:00\n"
     ]
    }
   ],
   "source": [
    "print(\"3600 seconds: %s\" %(time_from_seconds(3600)))\n",
    "print(\"0.04167 days: %s\" %(time_from_doy(0.04167)))"
   ]
  },
